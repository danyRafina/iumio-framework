  "config" :
  {
    "vendor-dir" : "./vendor/"
  },
  "require": {
    "php": ">=7.0.0",
    "smarty/smarty": "~3.1",
    "twbs/bootstrap": "3.3.7",
    "components/font-awesome" : "~4.7.0",
    "components/jquery" : "~3.2.1",
    "phpmailer/phpmailer": "~6.0"
  },
  "scripts": {
    "post-install-cmd": "mv ./vendor/twbs/bootstrap/dist/ ./public/components/libs/bootstrap && mv ./vendor/components/font-awesome/ ./public/components/libs/font-awesome/ &&  mv ./vendor/components/jquery/ ./public/components/libs/jquery/ &&  php ./vendor/iumio_framework/Core/Additional/Composer/Installer.php",
    "post-update-cmd":  "mv ./vendor/twbs/bootstrap/dist/ ./public/components/libs/bootstrap && mv ./vendor/components/font-awesome/ ./public/components/libs/font-awesome/ &&  mv ./vendor/components/jquery/ ./public/components/libs/jquery/ &&  php ./vendor/iumio_framework/Core/Additional/Composer/Installer.php"
  },
  "require-dev": {
    "mpyw/php-hyper-builtin-server": "^2.0"
  }
}
